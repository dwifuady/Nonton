@page "/"

<PageTitle>Discover</PageTitle>

@if (Addons is not null && Addons.Any())
{
    foreach (var addon in Addons)
    {
        var defaultCatalogs = addon.GetDefaultCatalogs()?.ToList();
        if (defaultCatalogs is not null && defaultCatalogs.Any())
        {
            foreach (var catalog in defaultCatalogs)
            {
                <ContentCards Addon="addon" Catalog="catalog"></ContentCards>
            }
        }
    }
}